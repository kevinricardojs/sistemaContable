<p id="notice"><%= notice %></p>

<h1>Listing Libro Compras</h1>

<table>
  <thead>
    <tr>
      <th>Establecimiento</th>
      <th>Mes</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @libro_compras.each do |libro_compra| %>
      <tr>
        <td><%= libro_compra.establecimiento %></td>
        <td><%= libro_compra.mes %></td>
        <td><%= libro_compra.year %></td>
        <td><%= link_to 'Show', libro_compra %></td>
        <td><%= link_to 'Edit', edit_libro_compra_path(libro_compra) %></td>
        <td><%= link_to 'Destroy', libro_compra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Libro compra', new_libro_compra_path %>
