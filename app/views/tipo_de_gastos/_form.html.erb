<%= form_for(@tipo_de_gasto) do |f| %>
<% if @tipo_de_gasto.errors.any? %>
<div class="card-panel">

  <p class="cyan-text text-darken-4">No se puede guardar la informaci√≥n:</p>

  <ul>
    <% @tipo_de_gasto.errors.full_messages.each do |message| %>
    <li class="red-text -text-darken-4"><%= message %></li>
    <% end %>
  </ul>
</div>
<% end %>

<div class="row">
  <div class="input-field col s4 offset-s4">
    <i class="material-icons prefix">receipt</i>
    <%= f.label :nombre %>
    <%= f.text_field :nombre %>
  </div>
  <div class="actions row">
    <div class="col s4 offset-s4 center-align">
      <%= f.submit class:"btn waves-effect waves-teal", value:"Enviar" %>
    </div>
  </div>
  <% end %>
