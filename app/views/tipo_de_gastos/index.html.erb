<p id="notice"><%= notice %></p>

<h1>Listing Tipo De Gastos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_de_gastos.each do |tipo_de_gasto| %>
      <tr>
        <td><%= tipo_de_gasto.nombre %></td>
        <td><%= link_to 'Show', tipo_de_gasto %></td>
        <td><%= link_to 'Edit', edit_tipo_de_gasto_path(tipo_de_gasto) %></td>
        <td><%= link_to 'Destroy', tipo_de_gasto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo de gasto', new_tipo_de_gasto_path %>
